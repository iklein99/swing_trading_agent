        {
          "name": "Get All Trades",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/api/trades?limit=50",
              "host": ["{{baseUrl}}"],
              "path": ["api", "trades"],
              "query": [
                {
                  "key": "limit",
                  "value": "50",
                  "description": "Number of trades to return"
                },
                {
                  "key": "symbol",
                  "value": "AAPL",
                  "description": "Filter by stock symbol",
                  "disabled": true
                },
                {
                  "key": "action",
                  "value": "BUY",
                  "description": "Filter by action (BUY or SELL)",
                  "disabled": true
